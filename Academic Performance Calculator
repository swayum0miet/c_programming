#include<stdio.h>
int main() {
    int a,b,c,d,e,total;
    float average,percentage;
    scanf("%d" , &a);
    scanf("%d" , &b);
    scanf("%d" , &c);
    scanf("%d" , &d);
    scanf("%d" , &e);
    total = a+b+c+d+e;
    average = total/5.0;
    percentage = (total/500.0)*100;
    printf("%d %.2f %.2f", total, average, percentage);

    return 0;
}
